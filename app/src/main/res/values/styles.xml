<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="tab_text" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">9dp</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:fontFamily">@font/apple_l</item>
    </style>

    <style name="tab_text02" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:fontFamily">@font/apple_m</item>
    </style>

    <style name="CustomRatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/WhiteGrey</item>
        <item name="colorControlActivated">@color/LightGrey</item>
    </style>

    <style name="Top_round">
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
    </style>

    <style name="round">
        <item name="cornerSize">18dp</item>
    </style>

    <style name="DatePickerDialog" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="android:background">@color/White</item>
        <item name="android:textColorPrimaryInverse">@color/Black</item>
        <item name="android:textColorPrimary">@color/Black</item>
        <item name="android:textColorSecondary">@color/SplashTigerOrange</item>
        <item name="android:textColorSecondaryInverse">@color/SplashTigerOrange</item>
        <item name="android:textColorTertiary">@color/Black</item>
        <item name="colorAccent">@color/SplashTigerOrange</item>
    </style>
</resources>