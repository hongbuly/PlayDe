<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/color_background"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="아이디 찾기"
        android:textColor="@color/White"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_searchid_back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_pass" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.96"
        app:srcCompat="@drawable/top_round_white" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고객님의 정보와 일치하는 아이디 목록입니다."
        android:layout_marginLeft="18dp"
        android:textSize="18dp"
        android:layout_marginTop="34dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <CheckBox
        android:id="@+id/checkBox_searchid2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/checkbox_color"
        android:button="@android:color/white"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/textView45"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="abc1234"
        android:textColor="@color/Black"
        android:layout_marginLeft="10dp"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox_searchid2"
        app:layout_constraintStart_toEndOf="@+id/checkBox_searchid2"
        app:layout_constraintTop_toTopOf="@+id/checkBox_searchid2" />
    <CheckBox
        android:id="@+id/checkBox2_searchid2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/checkbox_color"
        android:button="@android:color/white"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="@+id/textView45"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="abcd5678"
        android:textColor="@color/Black"
        android:layout_marginLeft="10dp"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox2_searchid2"
        app:layout_constraintStart_toEndOf="@+id/checkBox2_searchid2"
        app:layout_constraintTop_toTopOf="@+id/checkBox2_searchid2" />

    <Button
        android:id="@+id/btn_searchid2_login"
        android:layout_width="139dp"
        android:layout_height="41dp"
        android:background="@drawable/loginnext2_edge"
        android:text="로그인하기"
        android:layout_marginTop="566dp"
        android:layout_marginLeft="40dp"
        android:textStyle="bold"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <Button
        android:id="@+id/btn_searchid2_searchpass"
        android:layout_width="139dp"
        android:layout_height="41dp"
        android:background="@drawable/loginnext2_edge"
        android:text="비밀번호 찾기"
        android:layout_marginTop="566dp"
        android:textStyle="bold"
        android:layout_marginRight="40dp"
        android:textColor="@color/White"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />


</androidx.constraintlayout.widget.ConstraintLayout>